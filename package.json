{
  "name": "@swedev/ui",
  "version": "0.1.0",
  "description": "Shared, themeable UI components for swedev projects",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && npm run build:css",
    "build:css": "tailwindcss -i src/styles/tailwind.css -o dist/styles.css --minify",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@radix-ui/themes": "^3.0.0",
    "lucide-react": "^0.400.0"
  },
  "devDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "@radix-ui/themes": "^3",
    "lucide-react": "^0.469",
    "typescript": "^5.7",
    "tsup": "^8",
    "vitest": "^3",
    "@storybook/react-vite": "^10",
    "@storybook/addon-docs": "^10",
    "storybook": "^10",
    "tailwindcss": "^4",
    "@tailwindcss/cli": "^4",
    "@tailwindcss/vite": "^4",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swedev/ui"
  }
}
